		<dataSource>DataSource1</dataSource>
		<query>SELECT
	InventoryJournalBalance.Location AS Location,
	InventoryJournalBalance.Product.Type AS Type,
	InventoryJournalBalance.Product.Category AS Category,
	InventoryJournalBalance.Product.Ref AS Item,
	InventoryJournalBalance.Product.Description AS Description,
	InventoryJournalBalance.Product.PreferredVendor AS PreferredVendor,
	CASE
		WHEN InventoryJournalBalance.Product.Type = VALUE(Enum.InventoryTypes.Inventory)
			THEN InventoryJournalBalance.QuantityBalance
		WHEN InventoryJournalBalance.Product.Type = VALUE(Enum.InventoryTypes.NonInventory)
			THEN 0
		ELSE 0
	END AS QtyOnHand,
	InventoryJournalBalance.AmountBalance
FROM
	AccumulationRegister.InventoryJournal.Balance({(&amp;EndOfPeriod)}, ) AS InventoryJournalBalance</query>
