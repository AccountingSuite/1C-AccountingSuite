		<field xsi:type="DataSetFieldField">
			<dataPath>AmountTurnover</dataPath>
			<field>AmountTurnover</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>en</v8:lang>
					<v8:content>Profit</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>DataSource1</dataSource>
		<query>SELECT
	ProjectDataTurnovers.Project.Description,
	ProjectDataTurnovers.AmountReceipt,
	ProjectDataTurnovers.AmountExpense,
	ProjectDataTurnovers.AmountTurnover
FROM
	AccumulationRegister.ProjectData.Turnovers AS ProjectDataTurnovers
		LEFT JOIN Catalog.Projects AS Projects
		ON ProjectDataTurnovers.Project = Projects.Ref</query>
