		<dataSource>DataSource1</dataSource>
		<query>SELECT ALLOWED
	SystemTitle.Value AS OurCompany,
	CashFlowDataTurnovers.SalesPerson AS SalesPerson,
	CashFlowDataTurnovers.LineNumber AS LineNumber,
	CashFlowDataTurnovers.Recorder AS Recorder,
	CashFlowDataTurnovers.DayPeriod AS DayPeriod,
	CashFlowDataTurnovers.WeekPeriod AS WeekPeriod,
	CashFlowDataTurnovers.MonthPeriod AS MonthPeriod,
	CashFlowDataTurnovers.QuarterPeriod AS QuarterPeriod,
	CashFlowDataTurnovers.YearPeriod AS YearPeriod,
	CashFlowDataTurnovers.AmountRCTurnover AS AmountRCTurnover
{SELECT
	SalesPerson,
	LineNumber,
	Recorder,
	DayPeriod,
	WeekPeriod,
	MonthPeriod,
	QuarterPeriod,
	YearPeriod}
FROM
	AccumulationRegister.CashFlowData.Turnovers(
			,
			,
			Auto,
			Account.AccountType IN (VALUE(Enum.AccountTypes.Income), VALUE(Enum.AccountTypes.OtherIncome))
				
				OR Account = VALUE(ChartOfAccounts.ChartOfAccounts.EmptyRef)) AS CashFlowDataTurnovers
		LEFT JOIN Constant.SystemTitle AS SystemTitle
		ON (TRUE)
WHERE
	CashFlowDataTurnovers.Recorder REFS Document.CashReceipt
{WHERE
	CashFlowDataTurnovers.SalesPerson AS SalesPerson,
	CashFlowDataTurnovers.LineNumber AS LineNumber,
	CashFlowDataTurnovers.Recorder AS Recorder,
	CashFlowDataTurnovers.DayPeriod AS DayPeriod,
	CashFlowDataTurnovers.WeekPeriod AS WeekPeriod,
	CashFlowDataTurnovers.MonthPeriod AS MonthPeriod,
	CashFlowDataTurnovers.QuarterPeriod AS QuarterPeriod,
	CashFlowDataTurnovers.YearPeriod AS YearPeriod}</query>
