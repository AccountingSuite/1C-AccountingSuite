SELECT
	TT.Item,
	TT.ViewQty,
	TT.Order AS Order,
	TT.Company,
	SUM(TT.Qty) AS Qty
FROM
	TT AS TT
WHERE
	TT.Qty &lt;&gt; 0

GROUP BY
	TT.Item,
	TT.ViewQty,
	TT.Order,
	TT.Company

ORDER BY
	TT.Order.PointInTime</query>
