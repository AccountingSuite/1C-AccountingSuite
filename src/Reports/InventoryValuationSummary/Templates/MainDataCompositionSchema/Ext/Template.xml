		<dataSource>DataSource1</dataSource>
		<query>SELECT
	InventoryJournalBalance.Product AS Product,
	CAST(InventoryJournalBalance.QuantityBalance AS Number(22,4)) AS OnHand
INTO OnHand
FROM
	AccumulationRegister.InventoryJournal.Balance({(&amp;EndOfPeriod)}, ) AS InventoryJournalBalance
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	ItemLastCosts.Product AS Product,
	ItemLastCosts.Cost AS LastCost
INTO LastCost
FROM
	InformationRegister.ItemLastCosts.SliceLast({(&amp;EndOfPeriod)}, ) AS ItemLastCosts
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	InventoryJournalBalance.Product AS Product,
	CASE
		WHEN InventoryJournalBalance.QuantityBalance &lt;= 0
			THEN 0
		ELSE CASE
				WHEN InventoryJournalBalance.Product.PricePrecision = 3
					THEN CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 3))
				WHEN InventoryJournalBalance.Product.PricePrecision = 4
					THEN CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 4))
				ELSE CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 2))
			END
	END AS AverageCost,
	InventoryJournalBalance.AmountBalance AS AmountBalance
INTO AverageCost
FROM
	AccumulationRegister.InventoryJournal.Balance({(&amp;EndOfPeriod)}, ) AS InventoryJournalBalance
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	InventoryJournalBalance.Product AS Product,
	CASE
		WHEN InventoryJournalBalance.QuantityBalance &lt;= 0
			THEN 0
		ELSE CASE
				WHEN InventoryJournalBalance.Product.PricePrecision = 3
					THEN CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 3))
				WHEN InventoryJournalBalance.Product.PricePrecision = 4
					THEN CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 4))
				ELSE CAST(InventoryJournalBalance.AmountBalance / InventoryJournalBalance.QuantityBalance AS NUMBER(17, 2))
			END
	END AS Cost,
	InventoryJournalBalance.Layer AS Layer
INTO FIFOCostTT
FROM
	AccumulationRegister.InventoryJournal.Balance({(&amp;EndOfPeriod)}, ) AS InventoryJournalBalance
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	InventoryJournalBalance.Product AS Product,
	MIN(InventoryJournalBalance.Layer) AS Layer
INTO FIFOCostTTGroup
FROM
	AccumulationRegister.InventoryJournal.Balance({(&amp;EndOfPeriod)}, ) AS InventoryJournalBalance

GROUP BY
	InventoryJournalBalance.Product
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	FIFOCostTTGroup.Product AS Product,
	FIFOCostTTGroup.Layer,
	FIFOCostTT.Cost AS FIFOCost
INTO FIFOCost
FROM
	FIFOCostTTGroup AS FIFOCostTTGroup
		LEFT JOIN FIFOCostTT AS FIFOCostTT
		ON FIFOCostTTGroup.Product = FIFOCostTT.Product
			AND FIFOCostTTGroup.Layer = FIFOCostTT.Layer
;

////////////////////////////////////////////////////////////////////////////////
SELECT
	Products.Type,
	Products.Category,
	Products.Ref AS Item,
	Products.PreferredVendor,
	ISNULL(OnHandTT.OnHand, 0) AS OnHand,
	CASE
		WHEN Products.Ref.CostingMethod = VALUE(Enum.InventoryCosting.WeightedAverage)
			THEN ISNULL(AverageCostTT.AverageCost, 0)
		WHEN Products.Ref.CostingMethod = VALUE(Enum.InventoryCosting.FIFO)
			THEN ISNULL(FIFOCostTT.FIFOCost, 0)
		ELSE 0
	END AS AccountingCost,
	ISNULL(AverageCostTT.AverageCost, 0) AS AverageCost,
	ISNULL(AverageCostTT.AmountBalance, 0) AS AssetValue,
	ISNULL(LastCostTT.LastCost, 0) AS LastCost,
	Products.Price AS SalePrice,
	Products.Price * ISNULL(OnHandTT.OnHand, 0) AS RetailValue
FROM
	Catalog.Products AS Products
		LEFT JOIN OnHand AS OnHandTT
		ON Products.Ref = OnHandTT.Product
		LEFT JOIN LastCost AS LastCostTT
		ON Products.Ref = LastCostTT.Product
		LEFT JOIN AverageCost AS AverageCostTT
		ON Products.Ref = AverageCostTT.Product
		LEFT JOIN FIFOCost AS FIFOCostTT
		ON Products.Ref = FIFOCostTT.Product</query>
