			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogProjects.Ref,
	CatalogProjects.DeletionMark,
	CatalogProjects.Description,
	CatalogProjects.Customer,
	CatalogProjects.Internal,
	CatalogProjects.Predefined,
	CatalogProjects.PredefinedDataName,
	ProjectDataTurnovers.AmountTurnover,
	ProjectDataTurnovers.AmountReceipt,
	ProjectDataTurnovers.AmountExpense,
	SUM(TimeTrack.TimeComplete) AS Hours
FROM
	Catalog.Projects AS CatalogProjects
		LEFT JOIN Document.TimeTrack AS TimeTrack
		ON TimeTrack.Project = CatalogProjects.Ref
		LEFT JOIN AccumulationRegister.ProjectData.Turnovers AS ProjectDataTurnovers
		ON (ProjectDataTurnovers.Project = CatalogProjects.Ref)

GROUP BY
	CatalogProjects.Ref,
	CatalogProjects.DeletionMark,
	CatalogProjects.Description,
	CatalogProjects.Customer,
	CatalogProjects.Internal,
	CatalogProjects.Predefined,
	CatalogProjects.PredefinedDataName,
	ProjectDataTurnovers.AmountTurnover,
	ProjectDataTurnovers.AmountReceipt,
	ProjectDataTurnovers.AmountExpense</QueryText>
				<MainTable>Catalog.Projects</MainTable>
