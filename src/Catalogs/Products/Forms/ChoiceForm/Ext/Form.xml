	<Attributes>
		<Attribute name="List" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogProducts.Ref,
	CatalogProducts.DataVersion,
	CatalogProducts.DeletionMark,
	CatalogProducts.Predefined,
	CatalogProducts.Code,
	CatalogProducts.Description,
	CatalogProducts.CostingMethod,
	CatalogProducts.Type,
	CatalogProducts.InventoryOrExpenseAccount,
	CatalogProducts.IncomeAccount,
	CatalogProducts.COGSAccount,
	CatalogProducts.UM,
	CatalogProducts.DefaultLocation,
	CatalogProducts.CF1String,
	CatalogProducts.CF2String,
	CatalogProducts.CF3String,
	CatalogProducts.CF4String,
	CatalogProducts.CF5String,
	CatalogProducts.CF1Num,
	CatalogProducts.CF2Num,
	CatalogProducts.CF3Num,
	CatalogProducts.CF4Num,
	CatalogProducts.CF5Num,
	CatalogProducts.Category,
	CASE
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.Inventory)
			THEN CASE
					WHEN OrdersDispatchedBalance.QuantityBalance - OrdersDispatchedBalance.ReceivedBalance &gt; 0
						THEN OrdersDispatchedBalance.QuantityBalance - OrdersDispatchedBalance.ReceivedBalance
					ELSE 0
				END
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.NonInventory)
			THEN CASE
					WHEN OrdersDispatchedBalance.QuantityBalance - OrdersDispatchedBalance.InvoicedBalance &gt; 0
						THEN OrdersDispatchedBalance.QuantityBalance - OrdersDispatchedBalance.InvoicedBalance
					ELSE 0
				END
		ELSE 0
	END AS QtyOnPO,
	CASE
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.Inventory)
			THEN CASE
					WHEN OrdersRegisteredBalance.QuantityBalance - OrdersRegisteredBalance.ShippedBalance &gt; 0
						THEN OrdersRegisteredBalance.QuantityBalance - OrdersRegisteredBalance.ShippedBalance
					ELSE 0
				END
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.NonInventory)
			THEN CASE
					WHEN OrdersRegisteredBalance.QuantityBalance - OrdersRegisteredBalance.InvoicedBalance &gt; 0
						THEN OrdersRegisteredBalance.QuantityBalance - OrdersRegisteredBalance.InvoicedBalance
					ELSE 0
				END
		ELSE 0
	END AS QtyOnSO,
	CASE
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.Inventory)
			THEN InventoryJournalBalance.QuantityBalance
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.NonInventory)
			THEN 0
		ELSE 0
	END AS QtyOnHand,
	CASE
		WHEN CatalogProducts.Type = VALUE(Enum.InventoryTypes.Inventory)
			THEN ISNULL(InventoryJournalBalance.QuantityBalance, 0) + CASE
					WHEN ISNULL(OrdersDispatchedBalance.QuantityBalance, 0) - ISNULL(OrdersDispatchedBalance.ReceivedBalance, 0) &gt; 0
						THEN ISNULL(OrdersDispatchedBalance.QuantityBalance, 0) - ISNULL(OrdersDispatchedBalance.ReceivedBalance, 0)
					ELSE 0
				END - CASE
					WHEN ISNULL(OrdersRegisteredBalance.QuantityBalance, 0) - ISNULL(OrdersRegisteredBalance.ShippedBalance, 0) &gt; 0
						THEN ISNULL(OrdersRegisteredBalance.QuantityBalance, 0) - ISNULL(OrdersRegisteredBalance.ShippedBalance, 0)
					ELSE 0
				END
		ELSE 0
	END AS QtyAvailableToPromise
FROM
	Catalog.Products AS CatalogProducts
		LEFT JOIN AccumulationRegister.InventoryJournal.Balance AS InventoryJournalBalance
		ON CatalogProducts.Ref = InventoryJournalBalance.Product
		LEFT JOIN AccumulationRegister.OrdersDispatched.Balance AS OrdersDispatchedBalance
		ON CatalogProducts.Ref = OrdersDispatchedBalance.Product
		LEFT JOIN AccumulationRegister.OrdersRegistered.Balance AS OrdersRegisteredBalance
		ON CatalogProducts.Ref = OrdersRegisteredBalance.Product</QueryText>
				<MainTable>Catalog.Products</MainTable>
