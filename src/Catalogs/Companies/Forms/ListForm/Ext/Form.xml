			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	CatalogCompanies.Ref,
	CatalogCompanies.DeletionMark,
	CatalogCompanies.Code,
	CatalogCompanies.Description,
	CatalogCompanies.DefaultCurrency,
	CatalogCompanies.Vendor1099,
	CatalogCompanies.ExpenseAccount,
	CatalogCompanies.IncomeAccount,
	CatalogCompanies.Website,
	CatalogCompanies.Notes,
	CatalogCompanies.Terms,
	CatalogCompanies.StripeToken,
	CatalogCompanies.StripeID,
	CatalogCompanies.ARAccount,
	CatalogCompanies.APAccount,
	CatalogCompanies.PriceLevel,
	CatalogCompanies.Predefined,
	CatalogCompanies.PredefinedDataName,
	CatalogCompanies.CF1String,
	CatalogCompanies.CF2String,
	CatalogCompanies.CF3String,
	CatalogCompanies.CF4String,
	CatalogCompanies.CF5String,
	CatalogCompanies.CF1Num,
	CatalogCompanies.CF2Num,
	CatalogCompanies.CF3Num,
	CatalogCompanies.CF4Num,
	CatalogCompanies.CF5Num,
	CASE
		WHEN CatalogCompanies.Customer
				AND CatalogCompanies.Vendor
			THEN "C/V"
		WHEN CatalogCompanies.Customer
				AND NOT CatalogCompanies.Vendor
			THEN "Cust"
		WHEN NOT CatalogCompanies.Customer
				AND CatalogCompanies.Vendor
			THEN "Vend"
		ELSE ""
	END AS CV,
	ISNULL(GeneralJournalBalance.AmountRCBalance, 0) AS AmountRCBalance,
	CASE 
		WHEN zoho_accountCodeMap.zoho_id &lt;&gt; ""
			THEN TRUE
		ELSE FALSE
	END AS Zoho
FROM
	Catalog.zoho_accountCodeMap AS zoho_accountCodeMap
		RIGHT JOIN Catalog.Companies AS CatalogCompanies
			LEFT JOIN AccountingRegister.GeneralJournal.Balance AS GeneralJournalBalance
			ON (GeneralJournalBalance.ExtDimension1 = CatalogCompanies.Ref)
		ON zoho_accountCodeMap.company_ref = CatalogCompanies.Ref</QueryText>
