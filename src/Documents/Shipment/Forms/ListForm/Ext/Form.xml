			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	DocumentShipment.Ref,
	DocumentShipment.Number,
	DocumentShipment.Date,
	DocumentShipment.Company,
	DocumentShipment.LocationActual AS Location,
	CASE
		WHEN DocumentShipment.DeletionMark
			THEN VALUE(Enum.OrderStatuses.Deleted)
		WHEN NOT DocumentShipment.Posted
			THEN VALUE(Enum.OrderStatuses.Draft)
		WHEN OrdersStatuses.Status IS NULL 
			THEN VALUE(Enum.OrderStatuses.Open)
		WHEN OrdersStatuses.Status = VALUE(Enum.OrderStatuses.EmptyRef)
			THEN VALUE(Enum.OrderStatuses.Open)
		ELSE OrdersStatuses.Status
	END AS OrderStatus
FROM
	Document.Shipment AS DocumentShipment
		LEFT JOIN InformationRegister.OrdersStatuses.SliceLast AS OrdersStatuses
		ON DocumentShipment.Ref = OrdersStatuses.Order</QueryText>
