			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>SELECT
	AccumulationRegisterProjectData.Period,
	AccumulationRegisterProjectData.Recorder,
	AccumulationRegisterProjectData.LineNumber,
	AccumulationRegisterProjectData.Active,
	AccumulationRegisterProjectData.RecordType,
	AccumulationRegisterProjectData.Account,
	AccumulationRegisterProjectData.Project,
	AccumulationRegisterProjectData.Amount,
	AccumulationRegisterProjectData.PointInTime,
	CASE
		WHEN AccumulationRegisterProjectData.Recorder REFS Document.GeneralJournalEntry
			THEN VALUE(Catalog.Companies.EmptyRef)
		WHEN AccumulationRegisterProjectData.Recorder REFS Document.Deposit
			THEN ISNULL(DepositAccounts.Company, VALUE(Catalog.Companies.EmptyRef))
		ELSE ISNULL(AccumulationRegisterProjectData.Recorder.Company, VALUE(Catalog.Companies.EmptyRef))
	END AS Company
FROM
	AccumulationRegister.ProjectData AS AccumulationRegisterProjectData
		LEFT JOIN Document.Deposit.Accounts AS DepositAccounts
		ON AccumulationRegisterProjectData.Recorder = DepositAccounts.Ref
			AND AccumulationRegisterProjectData.LineNumber = DepositAccounts.LineNumber</QueryText>
				<MainTable>AccumulationRegister.ProjectData</MainTable>
